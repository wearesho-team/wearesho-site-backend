{
  "name": "wearesho-site-backend",
  "version": "1.0.0",
  "description": "Backend for Wearesho Studio Site",
  "main": "build/server.js",
  "repository": "git@github.com:wearesho-team/wearesho-site-backend.git",
  "scripts": {
    "test": "./node_modules/.bin/nyc mocha -r ts-node/register -r source-map-support/register tests/*-specs.ts",
    "prestart": "tsc --rootDir app --outDir build && babel build --out-dir build",
    "start": "node build/server.js"
  },
  "author": "Alexander Letnikow",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "body-parser": "^1.17.2",
    "class-validator": "^0.7.2",
    "cors": "^2.8.4",
    "counterpart": "^0.18.3",
    "express": "^4.15.4",
    "telebot": "^1.2.3",
    "typescript": "^2.5.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.4",
    "@types/chai": "^4.0.2",
    "@types/counterpart": "^0.18.0",
    "@types/express": "^4.0.36",
    "@types/mocha": "^2.2.41",
    "@types/telebot": "^1.2.1",
    "axios": "^0.16.2",
    "chai": "^4.1.1",
    "mocha": "^3.5.0",
    "nyc": "11.1.0",
    "ts-node": "^3.3.0"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "reporter": [
      "clover",
      "text-summary"
    ],
    "report-dir": "./tests/output"
  }
}
